# Navigate to the unzipped folder
cd path/to/agrestal_site

# Initialize Git
git init

# Add GitHub repo as remote
git remote add origin https://github.com/YOUR_USERNAME/agrestal-holdings-site.git

# Add files and commit
git add .
git commit -m "Initial commit - Agrestal Holdings website"

# Push to GitHub
git branch -M main
git push -u origin main
